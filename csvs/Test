<script>function fnShowMenu(id,divid)
{
var sobj = document.getElementById(id);
var x = findPosX(sobj);
var y = findPosY(sobj);
var ifr = document.getElementById("zc-dummyframe");
var mobj = document.getElementById("zc-homedropdown"+divid);
mobj.style.display="";
mobj.style.left = x-11;
mobj.style.top = y+20;
/*
ifr.style.left = x-11;
ifr.style.top = y+20;
ifr.style.width = mobj.offsetWidth + "px";
ifr.style.height = mobj.offsetHeight + "px";
ifr.style.display="";
*/
}
function fnRemMenu(divid)
{
document.getElementById("zc-homedropdown"+divid).style.display="none";
}</script><style type="text/css">
